{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [[
            "env", {
              "targets": {
                "electron": "1.8.2"
              }
            }],
          "react",
          "react-native"
        ],
        "plugins": ["transform-runtime", "react-native-web", "react-hot-loader/babel"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [[
          "env", {
            "targets": {
              "electron": "1.8.2"
            }
          }],
          "react",
          "react-native"
        ],
        "plugins": ["transform-runtime", "react-native-web"],
        "sourceMaps": "none"
      }
    }
  }
}